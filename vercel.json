{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "db.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/server.js"
        }
    ],
    "functions": {
        "api/server.js": {
            "memory": 1024,
            "includeFiles": "db.json"
        }
    }
}