{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/server.js"
        }
    ],
    "functions": {
        "api/server.js": {
            "memory": 1024,
            "includeFiles": "db.json"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "api/server.js"
        }
    ]
}