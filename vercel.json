{
    "functions": {
        "api/server.js": {
            "memory": 1024,
            "includeFiles": "db.json"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "api/server.js"
        }
    ],
    "rewrite": [
        {
            "source": "/app/(.*)",
            "destination": "/api/proxy"
        }
    ]
}